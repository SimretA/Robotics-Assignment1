<?xml version='1.0'?>
<sdf version='1.4'>
    <model name="arm_robot">
        <self_collide>1</self_collide>
        <link name="base">
            <pose>0 0 .05 0 0 0</pose>
            <gravity>0</gravity>
            <inertial>
                <mass>30</mass>
            </inertial>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.5</radius>
                        <length>.1</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.5</radius>
                        <length>.1</length>
                    </cylinder>
                </geometry>
                <material>
                    <script>
                        <uri>model://materials/scripts</uri>
                        <uri>model://materials/textures</uri>
                        <name>RepeatedTexture</name>
                    </script>
                </material>
            </visual>
        </link>
        <link name="arm1">
            <pose>0 0 0.125 0 0 0</pose>
            <gravity>0</gravity>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>.05</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>.05</length>
                    </cylinder>
                </geometry>
                <material>
                    <script>
                        <uri>model://materials/scripts</uri>
                        <uri>model://materials/textures</uri>
                        <name>RepeatedTexture</name>
                    </script>
                </material>
            </visual>
        </link>
        <link name="arm2">
            <pose>0 0 1.15 0 0 0</pose>
            <gravity>0</gravity>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>2</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>2</length>
                    </cylinder>
                </geometry>
                <material>
                    <script>
                        <uri>model://materials/scripts</uri>
                        <uri>model://materials/textures</uri>
                        <name>RepeatedTexture</name>
                    </script>
                </material>
            </visual>
        </link>
        <link name="arm3">
            <pose>0 0 2.65 0 0 0</pose>
            <gravity>0</gravity>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>1</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>1</length>
                    </cylinder>
                </geometry>
                <material>
                    <script>
                        <uri>model://materials/scripts</uri>
                        <uri>model://materials/textures</uri>
                        <name>RepeatedTexture</name>
                    </script>
                </material>
            </visual>
        </link>
        <link name="arm4">
            <pose>0 0 3.4 0 0 0</pose>
            <gravity>0</gravity>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.5</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.5</length>
